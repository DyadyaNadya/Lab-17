#include "pt4.h"
using namespace std;

#include <iterator>
#include <vector>
#include <deque>
#include <list>

void Solve()
{
    Task("STL2Seq2");
    list <int> L(ptin_iterator<int>(0), ptin_iterator<int>());
    copy(L.begin(), L.end(), ptout_iterator<int>());
    copy(L.rbegin(), L.rend(), ptout_iterator<int>());
   

}
